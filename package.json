    "test": "node test-server.js",
    "get-path": "node get-path.js",
    "example": "node example-usage.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PierrunoYT/replicate-imagen4-mcp-server.git"
  },
