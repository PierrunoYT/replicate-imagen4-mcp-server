{
  "name": "replicate-imagen4-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Replicate Imagen 4 Ultra - High-quality AI image generation with Google's latest model",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "replicate-imagen4": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
