  },
  "keywords": [
    "mcp",
    "mcp-server",
    "imagen4",
    "replicate",
    "image-generation",
    "ai",
    "google",
    "typescript",
    "model-context-protocol"
  ],
  "author": "PierrunoYT",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "replicate": "^0.34.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
